    "from scipy.spatial import distance as dist\n",
    "from imutils.video import VideoStream\n",
    "from imutils import face_utils\n",
    "from threading import Thread\n",
    "import numpy as np\n",
    "import playsound\n",
    "import argparse\n",
    "import imutils\n",
    "import time\n",
    "import dlib\n",
    "import cv2\n",
    "import datetime\n",
    "\n",
    "# From this various parts of the face :\n",
    "\n",
    "# The mouth can be accessed through points [48, 68].\n",
    "# The right eyebrow through points [17, 22].\n",
    "# The left eyebrow through points [22, 27].\n",
    "# The right eye using [36, 42].\n",
    "# The left eye with [42, 48].\n",
    "# The nose using [27, 36].\n",
    "# And the jaw via [0, 17].\n",
    "\n",
    "def eye_aspect_ratio(eye):\n",
    "    A = dist.euclidean(eye[1], eye[5])\n",
    "    B = dist.euclidean(eye[2], eye[4])\n",
    "    C = dist.euclidean(eye[0], eye[3])\n",
    "    ear = (A + B) / (2.0 * C)\n",
    "    return ear\n",
    "\n",
    "EYE_AR_THRESH = 0.3\n",
    "EYE_AR_CONSEC_FRAMES = 48\n",
    "COUNTER = 0\n",
    "ALARM_ON = False\n",
    "\n",
    "def sound_alarm(path):\n",
    "    playsound.playsound(path)\n",
    "    \n",
    "detector = dlib.get_frontal_face_detector()\n",
    "predictor = dlib.shape_predictor('C:/Users/ChungkiLee/Documents/GitHub/webcam/previewimgs/shape_predictor_68_face_landmarks.dat')\n",
    "\n",
    "(lStart, lEnd) = face_utils.FACIAL_LANDMARKS_IDXS[\"left_eye\"]\n",
    "(rStart, rEnd) = face_utils.FACIAL_LANDMARKS_IDXS[\"right_eye\"]\n",
    "\n",
    "vs = VideoStream(src=0).start()\n",
    "time.sleep(1.0)\n",
    "\n",
    "(dt, micro) = datetime.datetime.now().strftime('%Y%m%d%H%M%S.%f').split('.')\n",
    "b = float(dt) + float(micro)/1000000\n",
    "time_x = []\n",
    "while True:\n",
    "    frame = vs.read()\n",
    "    frame = imutils.resize(frame, width=720)\n",
    "    frame = cv2.flip(frame, 1)\n",
    "    gray  = cv2.cvtColor(frame, cv2.COLOR_BGR2GRAY)\n",
    "    rects = detector(gray, 0)\n",
    "    for rect in rects:\n",
    "        shape0 = predictor(gray, rect)\n",
    "        shape0 = face_utils.shape_to_np(shape0)\n",
    "        \n",
    "        mg = 3\n",
    "#         print(np.array(shape0).shape)\n",
    "#         print([np.min(np.array(shape0)[:,0]), np.min(np.array(shape0)[:,1])])\n",
    "#         print('--------------------------------')\n",
    "        x,y,w,h = [np.min(shape0[:, 0]), np.min(shape0[:, 1]), np.max(shape0[:, 1])-np.min(shape0[:, 1]), np.max(shape0[:, 0])-np.min(shape0[:, 0])]\n",
    "#         print('--------------------------------')\n",
    "        cv2.rectangle(frame,(x-mg-2,y-mg-2),(x+w+mg+1,y+h+mg+1),(0,255,0),1)\n",
    "        for (i, (x, y)) in enumerate(shape0):\n",
    "            cv2.circle(frame, (x, y), 2, (0, 0, 255), 1)\n",
    "        leftEye = shape0[lStart:lEnd]\n",
    "        rightEye = shape0[rStart:rEnd]\n",
    "        leftEAR = eye_aspect_ratio(leftEye)\n",
    "        rightEAR = eye_aspect_ratio(rightEye)\n",
    "        ear = (leftEAR + rightEAR) / 2.0\n",
    "        leftEyeHull = cv2.convexHull(leftEye)\n",
    "        rightEyeHull = cv2.convexHull(rightEye)\n",
    "        cv2.drawContours(frame, [leftEyeHull], -1, (0, 255, 0), 1)\n",
    "        cv2.drawContours(frame, [rightEyeHull], -1, (0, 255, 0), 1)\n",
    "#         if ear < EYE_AR_THRESH:\n",
    "#             COUNTER += 1\n",
    "#             if COUNTER >= EYE_AR_CONSEC_FRAMES:\n",
    "#                 if not ALARM_ON:\n",
    "#                     ALARM_ON = True\n",
    "#                 cv2.putText(frame, \"DROWSINESS ALERT!\", (10, 30),\n",
    "#                     cv2.FONT_HERSHEY_SIMPLEX, 0.7, (0, 0, 255), 2)\n",
    "#         else:\n",
    "#             COUNTER = 0\n",
    "#             ALARM_ON = False\n",
    "#         cv2.putText(frame, \"EAR: {:.2f}\".format(ear), (300, 30),\n",
    "#             cv2.FONT_HERSHEY_SIMPLEX, 0.7, (0, 0, 255), 2)\n",
    " \n",
    "    cv2.imshow(\"Frame\", frame)\n",
    "    (dt, micro) = datetime.datetime.now().strftime('%Y%m%d%H%M%S.%f').split('.')\n",
    "    a = float(dt) + float(micro)/1000000\n",
    "    time_x.append(a-b)\n",
    "    if a-b >= 10:\n",
    "        break\n",
    "    key = cv2.waitKey(1) & 0xFF\n",
    "    if key == ord(\"q\"):\n",
    "        break\n",
    "\n",
    "\n",
    "# Closes all the frames\n",
    "cv2.destroyAllWindows()\n",
    "vs.stop()\n",
    "\n",
    "tmp = np.array(time_x)\n",
    "print(tmp.shape)\n",
    "print(tmp[0])\n",
    "print(tmp[-1])"
   ]
